<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<unique-id>1657897673227330-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>catfish</icon>
	<name>Search</name>
	<unique-id>1658187036027807-1</unique-id>
	<command>catfish --path=%f</command>
	<description>Search the files in the directory recursively</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>visual-studio-code</icon>
	<name>Open in VS Code</name>
	<unique-id>1658244389026235-1</unique-id>
	<command>code %f</command>
	<description>Open the selected directory with VS Code</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>emblem-symbolic-link</icon>
	<name>Create symlink</name>
	<unique-id>1658264593976018-1</unique-id>
	<command>ln -Ts %f %n&quot; (symlink)&quot;</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
</action>
<action>
	<icon>thunar</icon>
	<name>Open thunar as root here</name>
	<unique-id>1658265118037734-3</unique-id>
	<command>sudo thunar %f</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>group-new</icon>
	<name>Move into new folder</name>
	<unique-id>1658265334433976-4</unique-id>
	<command>NEW_FOLDER=$(/usr/bin/zenity --entry --title &apos;Move into new folder&apos; --text &apos;Please enter a name for the new folder&apos;) &amp;&amp; mkdir &quot;$NEW_FOLDER&quot; &amp;&amp; mv %F &quot;$NEW_FOLDER&quot;</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
</actions>
