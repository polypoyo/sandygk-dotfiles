#!/usr/bin/env fish

abbr -g rf '. ~/.config/fish/config.fish'
abbr -g rm 'rm -i'
abbr -g q exit
abbr -g cl clear
abbr -g bc 'bc -q'
abbr -g mi 'sudo make install'
abbr -g cbs 'create_bin_script'
abbr -g md 'sudo mount octoking.local:/srv/nfs/data /home/sandy/data'
abbr -g umd 'sudo umount /home/sandy/data'
abbr -g make_executable 'chmod u+x'
