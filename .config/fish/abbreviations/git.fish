#!/usr/bin/env fish

abbr -g gc 'git clone'
abbr -g gca 'git commit -am'
abbr -g gcb 'git checkout -b'
abbr -g gps 'git push -u origin HEAD'
abbr -g gpl 'git pull'
abbr -g gsu 'git stash --include-untracked'
abbr -g gsp 'git stash pop'
abbr -g gcl 'git clone'
abbr -g gcp 'git cherry-pick'
abbr -g gin 'git init'
